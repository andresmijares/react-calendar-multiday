{
	"name": "react-calendar-multiday",
	"version": "0.0.18",
	"description": "A minimalist multiday calendar by Shiftgig",
	"main": "lib/app.min.js",
	"scripts": {
		"build": "webpack --env build",
		"dev": "webpack --progress --colors --watch --env dev",
		"test": "bundlesize",
		"serve": "webpack-dev-server --host 0.0.0.0 --config ./webpack.config.js --env dev"
	},
	"keywords": [
		"react",
		"react-calendar-multiday",
		"calendar"
	],
	"author": "andresmijares, ssaucedo",
	"license": "MIT",
	"jest": {
		"globals": {
			"__DEV__": true
		},
		"moduleNameMapper": {
			"\\.(css|scss)$": "identity-obj-proxy"
		},
		"verbose": false,
		"testEnvironment": "node"
	},
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-core": "^6.7.6",
		"babel-eslint": "next",
		"babel-jest": "^18.0.0",
		"babel-loader": "^7.1.1",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-lodash": "^3.2.9",
		"babel-plugin-module-resolver": "^2.3.0",
		"babel-plugin-react-transform": "^3.0.0",
		"babel-plugin-transform-es2015-classes": "^6.23.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
		"babel-plugin-transform-imports": "^1.4.0",
		"babel-plugin-transform-runtime": "^6.15.0",
		"babel-polyfill": "^6.16.0",
		"babel-preset-env": "^1.1.4",
		"babel-preset-es2015": "^6.6.0",
		"babel-preset-react": "^6.5.0",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.16.0",
		"bundlesize": "^0.15.2",
		"css-loader": "^0.28.7",
		"eslint": "3.19.0",
		"eslint-loader": "1.7.1",
		"extract-text-webpack-plugin": "^3.0.1",
		"html-webpack-plugin": "^2.30.1",
		"style-loader": "^0.19.0",
		"webpack": "3.1.0",
		"webpack-dev-server": "^2.9.1",
		"yargs": "7.1.0"
	},
	"dependencies": {
		"moment": "^2.18.1",
		"moment-range": "^3.0.3",
		"ramda": "^0.24.1",
		"react": "^15.5.4",
		"react-dom": "15.5.1"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/sgrepo/react-calendar-multiday.git"
	},
	"bundlesize": [
		{
			"path": "./lib/app.min.js",
			"maxSize": "17 kB"
		}
	]
}
