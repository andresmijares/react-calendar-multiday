    "css-loader": "^0.28.7",
    "eslint": "3.19.0",
    "eslint-loader": "1.7.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "html-webpack-plugin": "^2.30.1",
    "react": "^16.0.0",
    "react-dom": "16.0.0",
    "style-loader": "^0.19.0",
    "webpack": "3.1.0",
    "webpack-dev-server": "^2.9.1",
