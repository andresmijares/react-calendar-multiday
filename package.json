{
	"name": "react-multiday-calendar",
	"version": "1.0.0",
	"description": "A minimalist multiday calendar by Shiftgig",
	"main": "index.js",
	"scripts": {
		"start": "npm-run-all --parallel serve:dev lint:watch",
		"serve:dev": "webpack-dev-server --host 0.0.0.0 --config ./webpack.config.babel.js",
		"build": "yarn run lint && yarn run babel-node server/build && exit 0;",
		"lint": "esw webpack.config.babel.js app --color",
		"lint:watch": "yarn run lint -- --watch",
		"test:dev": "jest --watchAll",
		"test": "jest --bail --coverage --verbose",
		"test:commit": "jest --bail --notify"
	},
	"keywords": [
		"react",
		"react-calendar",
		"calendar"
	],
	"author": "andresmijares, ssaucedo",
	"license": "MIT",
	"pre-commit": {
		"run": [
			"lint"
		],
		"colors": true
	},
	"jest": {
		"globals": {
			"__DEV__": true
		},
		"verbose": false,
		"testEnvironment": "node"
	},
	"devDependencies": {
		"babel-cli": "^6.18.0",
		"babel-core": "^6.7.6",
		"babel-eslint": "next",
		"babel-jest": "^18.0.0",
		"babel-loader": "^7.1.1",
		"babel-plugin-add-module-exports": "^0.2.1",
		"babel-plugin-lodash": "^3.2.9",
		"babel-plugin-module-resolver": "^2.3.0",
		"babel-plugin-transform-es2015-classes": "^6.23.0",
		"babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
		"babel-plugin-transform-imports": "^1.4.0",
		"babel-plugin-transform-react-constant-elements": "^6.9.1",
		"babel-plugin-transform-react-remove-prop-types": "^0.2.10",
		"babel-plugin-transform-runtime": "^6.15.0",
		"babel-polyfill": "^6.16.0",
		"babel-preset-env": "^1.1.4",
		"babel-preset-es2015": "^6.6.0",
		"babel-preset-react": "^6.5.0",
		"babel-preset-react-hmre": "^1.1.1",
		"babel-preset-stage-0": "^6.16.0",
		"browser-sync": "^2.17.6",
		"bs-fullscreen-message": "^1.1.0",
		"bulma-loader": "^1.0.1",
		"chalk": "^1.1.3",
		"chunk-manifest-webpack-plugin": "^1.1.0",
		"compression-webpack-plugin": "^1.0.0",
		"connect-history-api-fallback": "^1.3.0",
		"copy-webpack-plugin": "^4.0.1",
		"css-loader": "^0.26.1",
		"env2": "^2.1.1",
		"enzyme": "^3.0.0",
		"enzyme-to-json": "^3.0.1",
		"eslint": "^3.10.0",
		"eslint-config-standard": "^6.2.1",
		"eslint-plugin-promise": "^3.3.2",
		"eslint-plugin-react": "^7.0.1",
		"eslint-plugin-standard": "^2.0.1",
		"eslint-watch": "^2.1.14",
		"extract-text-webpack-plugin": "^3.0.0",
		"file-loader": "^0.9.0",
		"html-webpack-plugin": "^2.24.1",
		"identity-obj-proxy": "^3.0.0",
		"inline-manifest-webpack-plugin": "^3.0.1",
		"jest-cli": "^18.1.0",
		"jsdom": "^9.9.1",
		"json-loader": "^0.5.4",
		"last-release-git": "^0.0.2",
		"node-sass": "^4.5.0",
		"npm-check": "^5.4.0",
		"npm-run-all": "^3.1.1",
		"nsp": "^2.6.2",
		"postcss-loader": "^1.1.0",
		"pre-commit": "^1.1.3",
		"prop-types": "^15.5.10",
		"react-addons-test-utils": "^15.6.2",
		"react-test-renderer": "^16.0.0",
		"resolve-url": "^0.2.1",
		"rimraf": "^2.5.4",
		"sass-loader": "^6.0.6",
		"strip-ansi": "^3.0.1",
		"style-loader": "^0.13.1",
		"uglifyjs-webpack-plugin": "beta",
		"url-loader": "^0.5.7",
		"webpack": "^3.6.0",
		"webpack-dev-server": "^2.8.2",
		"webpack-manifest-plugin": "^1.1.0",
		"webpack-md5-hash": "^0.0.5"
	},
	"dependencies": {
		"moment": "^2.18.1",
		"moment-range": "^3.0.3",
		"ramda": "^0.24.1",
		"react": "^16.0.0",
		"react-dom": "16.0.0"
	}
}
